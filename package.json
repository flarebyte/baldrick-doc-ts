{
  "name": "baldrick-doc-ts",
  "description": "fixme",
  "keywords": ["cli"],
  "version": "0.6.0",
  "author": {
    "name": "Olivier Huin",
    "url": "https://github.com/olih"
  },
  "license": "MIT",
  "homepage": "https://github.com/flarebyte/baldrick-doc-ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/flarebyte/baldrick-doc-ts.git"
  },
  "bugs": "https://github.com/flarebyte/baldrick-doc-ts/issues",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/src/cli.mjs",
      "default": "./dist/src/cli.mjs",
      "types": "./dist/src"
    },
    "./package.json": {
      "default": "./package.json"
    }
  },
  "files": [
    "dist/src",
    "src"
  ],
  "bin": {
    "baldrick-doc-ts": "dist/src/cli.mjs"
  },
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "lint": "baldrick lint check",
    "lint:fix": "baldrick lint fix",
    "lint:ci": "baldrick lint ci",
    "test": "baldrick test check",
    "test:fix": "baldrick test fix",
    "test:cov": "baldrick test cov",
    "test:ci": "baldrick test ci",
    "reset": "rm -rf dist; rm -rf report",
    "build": "yarn reset;tsc --outDir dist",
    "ready": "yarn lint && yarn test:cov && yarn outdated",
    "norm": "npx baldrick-ts generate -f cli -ga 'flarebyte' -ch 'Flarebyte.com' -cy 2021 -l MIT -b baldrick-doc-ts",
    "norm:g": "baldrick-ts generate -f cli -ga 'flarebyte' -ch 'Flarebyte.com' -cy 2021 -l MIT -b baldrick-doc-ts",
    "github": "gh repo edit --delete-branch-on-merge --enable-squash-merge"
  },
  "dependencies": {},
  "devDependencies": {
    "baldrick-dev-ts": "^0.8.5",
    "typescript": "^4.5.4"
  },
  "peerDependencies": {}
}
